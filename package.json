{
  "name": "@cpanel/api",
  "version": "3.0.2",
  "description": "cPanel Api JavaScript and TypeScript interface libraries. This library provides a set of classes for calling cPanel Whm API 1 and UAPI calls. The classes hide much of the complexity of these API's behind classes the abstract the underlying variances between these API systems. Users of this library can focus on what they want to accomplish rather the having to learn the various complexities of the underlying wire formats for each of the cPanel Products APIs",
  "main": "dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "*.d.ts",
    "*.js",
    "*.map.js",
    "**/*.d.ts",
    "**/*.js",
    "**/*.map.js",
    "documentation/**/*"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:ts && yarn webpack:prod && yarn webpack:dev",
    "build:dev": "yarn clean && yarn build:ts-dev && yarn webpack:dev",
    "build:ts": "tsc -p tsconfig.prod.json",
    "build:ts-dev": "tsc -p tsconfig.dev.json",
    "webpack:dev": "webpack --env=dev",
    "webpack:prod": "webpack --env=prod",
    "clean": "rm -rf dist",
    "build-docs": "compodoc -p tsconfig.dev.json -d ./docs",
    "serve-docs": "compodoc -p tsconfig.dev.json -d ./docs -r 4214 -s",
    "test": "yarn run build:ts-dev && jasmine '**/*.spec.js'",
    "test:cover": "nyc -e .ts -x '**/*.spec.ts' jasmine-ts --config=jasmine.json && nyc report -r lcov",
    "ts-tidy": "npx eslint -c .ts-eslint-tidy-rc.json 'src/**/*.ts' --fix",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CpanelInc/cpanel-node-api"
  },
  "author": "cPanel L.L.C.",
  "contributors": [
    {
      "name": "Aneece Yazdani",
      "email": "aneece@cpanel.net"
    },
    {
      "name": "Sruthi Sanigarapu",
      "email": "sruthi@cpanel.net"
    },
    {
      "name": "Thomas Green",
      "email": "tomg@cpanel.net"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CpanelInc/cpanel-node-api/issues"
  },
  "homepage": "https://cpanel.com",
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "2.6.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "http-server": "^0.11.1",
    "jasmine": "2.8.0",
    "jasmine-ts": "^0.3.0",
    "nyc": "^14.1.1",
    "readme-generator": "^1.0.1",
    "ts-node": "^8.10.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "publishConfig": {
    "registry": "https://vmware-manager.dev.cpanel.net/verdaccio/"
  },
  "keywords": [
    "api",
    "uapi",
    "whmapi"
  ],
  "dependencies": {
    "@types/lodash": "^4.14.121",
    "@types/node": "8.0.31",
    "lodash": "^4.17.11",
    "tslib": "^1.14.1",
    "typescript": "3.8.3"
  }
}
