{
  "name": "@cpanel/api",
  "version": "1.1.1-alpha.0",
  "description": "cPanel Api JavaScript and TypeScript libraries",
  "main": "index.js",
  "files": [
    "*.d.ts",
    "*.js",
    "*.map.js",
    "**/*.d.ts",
    "**/*.js",
    "**/*.map.js",
    "documentation/**/*"
  ],
  "scripts": {
    "build": "yarn clean && yarn build:ts && yarn webpack:prod && yarn webpack:dev",
    "build:dev": "yarn clean && yarn build:ts && yarn webpack:dev",
    "build:ts": "tsc -p tsconfig.json",
    "webpack:dev": "webpack --env=dev",
    "webpack:prod": "webpack --env=prod",
    "clean": "rm -rf dist",
    "build-docs": "compodoc -p tsconfig.json -d ./docs",
    "serve-docs": "compodoc -p tsconfig.json -d ./docs -r 4214 -s",
    "test": "yarn run build:ts && jasmine '**/*.spec.js'",
    "test:cover": "nyc -e .ts -x '**/*.spec.ts' jasmine-ts --config=jasmine.json && nyc report -r lcov",
    "link:cpanel": "yarn link && cd /usr/local/cpanel/base/frontend/paper_lantern/ng-apps-src/ && yarn link \"@cpanel/api\"",
    "link:webmail": "yarn link && cd /usr/local/cpanel/base/webmail/paper_lantern/ng-apps-src/ && yarn link \"@cpanel/api\"",
    "link:whm": "yarn link && cd /usr/local/cpanel/whostmgr/docroot/templates/ng-apps-src/ && yarn link \"@cpanel/api\"",
    "ts-tidy": "npx eslint -c .ts-eslint-tidy-rc.json 'src/**/*.ts' --fix"
  },
  "repository": "ssh://git@enterprise.cpanel.net:7999/cn/api.git",
  "author": "Team Phoenix @ cPanel",
  "contributors": [
    "Aneece Yazdani <aneece@cpanel.net>",
    "Sruthi Sanigarapu <sruthi@cpanel.net>",
    "Thomas Green <tomg@cpanel.net>"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.10",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint": "^6.5.1",
    "http-server": "^0.11.1",
    "jasmine-ts": "^0.3.0",
    "readme-generator": "^1.0.1",
    "ts-node": "^8.4.1",
    "webpack": "^4.39.0",
    "webpack-cli": "^3.3.6"
  },
  "publishConfig": {
    "registry": "https://vmware-manager.dev.cpanel.net/verdaccio/"
  },
  "keywords": [
    "api",
    "uapi",
    "whmapi"
  ],
  "dependencies": {
    "@types/jasmine": "2.6.0",
    "@types/lodash": "^4.14.121",
    "@types/node": "8.0.31",
    "jasmine": "2.8.0",
    "lodash": "^4.17.11",
    "nyc": "^14.1.1",
    "tslib": "^1.9.3",
    "typescript": "3.2.2"
  }
}
